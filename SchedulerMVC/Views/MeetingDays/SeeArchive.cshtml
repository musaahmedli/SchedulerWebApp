@model SchedulerMVC.Models.MeetingWeekModels.ArchiveWeekModel

<style>
    td {
        text-align: center;
        color: aliceblue;
    }

    th {
        text-align: center;
        color: aliceblue;
    }
</style>

<div class="container d-block mb-3">
    <div class="container"><h2 class="text-capitalize text-center text-light">Arxiv</h2></div>
    <div class="d-block mb-3">

    </div>


</div>
@foreach (var item in Model.archiveWeeks)
{
    <div class="mt-3">
        <table class="table-dark table-hover table-responsive ">
            <thead>
                <tr>
                    <th>Tarix</th>
                    <th>Gün</th>
                </tr>
            </thead>
            <tbody>

                @foreach (var days in item.MeetingDays)
                {
                    <tr>
                        <td class="col">@item.StartDate.AddDays(days.DayId - 1).ToString("MM/dd/yyyy")</td>
                        <td class="col">@days.Days.Day</td>

                        <td class="col"><button class="btn btn-light">@Html.ActionLink("Görüşlər", "Index", "Meeting", new { meetingDaysId = days.MeetingDaysId, employeeId = Model.EmployeeId })</button></td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
}